import numpy as np
import matplotlib.pyplot as plt
# Crear un array de 10x10 lleno de ceros
MAZE = np.zeros((10, 10))
MAZE[0, :] = 1
MAZE[-1, :] = 1
MAZE[:, 0] = 1
MAZE[:, -1] = 1

def visualize_example(x): 
    plt.figure() 
    plt.imshow(x) 
    plt.colorbar() 
    plt.grid(False) 
    plt.show()
visualize_example(MAZE)